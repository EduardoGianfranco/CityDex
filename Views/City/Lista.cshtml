@model List<City>

<h1>Lista de Ciudades registradas en la CityDex</h1>

@if (Model.Count == 0){
    <div class="alert alert-warning text-center">La lista esta vac√≠a... Por ahora</div>
}else{
<ol>
@foreach (var p in Model){
    <li class="d-flex">
        <div>Ciudad: @p.Nombre</div>
        <div>  Lider del Gimnasio Local: @p.LiderG</div>
    </li>
}
</ol>
}